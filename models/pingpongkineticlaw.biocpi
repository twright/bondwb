species A = a -> P;
species B = b -> Q;
species E = e -> E;
species P = p -> P;
species Q = q -> Q;

kinetic law PingPong(Vmax, KA, KB; A, B, E) = Vmax * A * B /(KA*B + KB*A + A*B);

affinity network M = {
	a,b,e at rate PingPong(1,2,2);
}

process Pi = [1.0] E || [5.0] A || [5.0] B || [0.0] P || [0.0] Q with network M;
