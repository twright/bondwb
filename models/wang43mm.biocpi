species RP = rp -> RPP;

species RPP = rpp -> RP;

species S = s -> S;

species A = a -> A;

process P = [1.0] RPP || [0.01] S || [0.0] RP || [0.01] A
            with network Aff;

kinetic law MichaelisMenten(k, K; A, B) = k * A * B / (K + B);

affinity network Aff = {
  a, rp at rate MichaelisMenten(0.01, 0.05);
  s, rpp at rate MichaelisMenten(100, 0.05);
}
